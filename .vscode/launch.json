{
  "version": "0.2.0",
"configurations": [
    {
        "name": "employees-config1",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/employees",
        "runtimeExecutable": "npx",
        "windows": {
            "runtimeExecutable": "npx.cmd"
        },
        "runtimeArgs": [
            "fiori",
            "run"
        ],
        "args": [
            "--open",
            "index.html"
        ],
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
            "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/sapui5_advanced/employees\"}",
            "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
        }
    }
]
}